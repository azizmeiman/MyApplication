<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegExpEmp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="509dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="2dp"
        android:orientation="vertical">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="370dp"
                    android:layout_height="79dp"
                    app:srcCompat="@drawable/logop" />

                <EditText
                    android:id="@+id/EmailP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:hint="البريد الألكتروني"
                    android:inputType="" />

                <EditText
                    android:id="@+id/PasswordP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:hint="الرقم السري"
                    android:inputType="textPassword" />

                <EditText
                    android:id="@+id/namep"
                    android:layout_width="match_parent"
                    android:layout_height="43dp"
                    android:ems="10"
                    android:hint="الاسم الثلاثي"
                    android:inputType="" />

                <EditText
                    android:id="@+id/RPIDP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:hint="رقم هوية المعرف"
                    android:inputType="text" />

                <EditText
                    android:id="@+id/RPphoneNumP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:hint="رقم الجوال"
                    android:inputType="text" />
                <Spinner
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/cityPoster"
                    android:textSize="20sp"

                    />


                <EditText
                    android:id="@+id/orgNameP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:hint="اسم المنظمة"
                    android:inputType="text" />

                <EditText
                    android:id="@+id/orgDocP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:hint="وثائق المنظمة"
                    android:inputType="text" />

                <EditText
                    android:id="@+id/orgPicP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:hint="صورة المنظمة"
                    android:inputType="text" />

            </LinearLayout>
        </ScrollView>


    </LinearLayout>

    <Button
        android:id="@+id/RegE"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="0dp"
        android:background="@color/colorPrimary"
        android:text="@string/RegisterAr"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" />



</RelativeLayout>